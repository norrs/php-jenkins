{
    "name": "php-jenkins/php-example",
    "description": "php example",
    "license": "MIT",

    "repositories": [
    ],

    "require": {
        "ext-memcache": ">=3",
        "php": ">=5.6",
        "docopt/docopt": "0.6.0",
        "zendframework/zend-diactoros": "^1.3"
    },

    "require-dev": {
        "ext-xdebug": "2.*",
        "phpunit/phpunit": ">=4.5.0",
        "squizlabs/php_codesniffer": ">=2.0.0",
        "phploc/phploc": "*",
        "phpmd/phpmd" : ">=2.1.3",
        "sebastian/phpcpd": ">=2.0.1",
        "ukko/phpredis-phpdoc": "*",
        "fabpot/php-cs-fixer": "*",
        "apigen/apigen": "^4.1",
        "deployer/deployer": ">=3.0"
    },

    "preferred-install": "dist",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "notify-on-install": false,

    "autoload": {
        "classmap": [
            "src/main"
        ]
    }
}
